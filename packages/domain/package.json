{
  "name": "cmdo-domain",
  "version": "0.2.0-dev",
  "description": "Event Sourced DDD+CQRS Domain Module.",
  "repository": "https://github.com/cmdo/toolkit.git",
  "author": "Christoffer RÃ¸dvik <dev@kodemon.net>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "keywords": [
    "es",
    "ddd",
    "cqrs",
    "server",
    "web"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "yarn clean && yarn && yarn compile",
    "clean": "rm -rf ./dist ./node_modules ./tsconfig.tsbuildinfo",
    "compile": "../../node_modules/.bin/tsc -p tsconfig.json && ../../node_modules/.bin/tsc -p tsconfig-esm.json",
    "watch": "./node_modules/.bin/nodemon",
    "test": "node --inspect ../../node_modules/.bin/jest --runInBand --verbose",
    "lint": "../../node_modules/.bin/eslint 'src/**/*.ts'"
  },
  "dependencies": {
    "cmdo-access": "1.1.1",
    "cmdo-inverse": "1.1.1",
    "crypto-js": "4.0.0",
    "eventemitter3": "4.0.7",
    "nanoid": "3.1.22"
  },
  "devDependencies": {
    "@types/crypto-js": "4.0.1",
    "@types/eventemitter3": "2.0.2",
    "nodemon": "2.0.7"
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "ext": "ts",
    "exec": "yarn compile"
  }
}
